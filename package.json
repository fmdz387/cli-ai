{
  "name": "@fmdzc/cli-ai",
  "version": "3.2.2",
  "description": "CLI AI Assistant v3 - Natural language to shell commands with persistent REPL session",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "s": "./dist/cli.js",
    "cli-ai": "./dist/cli.js"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:security": "eslint src --rule 'security/*: error'",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "test": "vitest run",
    "test:watch": "vitest",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "security": "pnpm audit && pnpm lint:security",
    "prepare": "tsup",
    "prepublishOnly": "pnpm typecheck && pnpm audit --audit-level=high && pnpm build",
    "link:global": "pnpm build && pnpm link --global",
    "unlink:global": "pnpm unlink --global",
    "uninstall": "node scripts/uninstall.js",
    "uninstall:force": "node scripts/uninstall.js --force",
    "version:set": "node scripts/version.js",
    "tag": "node scripts/tag.js",
    "tag:repush": "node scripts/tag.js --repush"
  },
  "keywords": [
    "cli",
    "ai",
    "assistant",
    "terminal",
    "shell",
    "anthropic",
    "claude",
    "natural-language",
    "command-line"
  ],
  "author": "fmdz387",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmdz387/cli-ai.git"
  },
  "homepage": "https://github.com/fmdz387/cli-ai#readme",
  "bugs": {
    "url": "https://github.com/fmdz387/cli-ai/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "contributors": [
    {
      "name": "fmdz387",
      "url": "https://github.com/fmdz387"
    },
    {
      "name": "fmdz387",
      "url": "https://x.com/fmdz387"
    }
  ],
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@napi-rs/keyring": "^1.2.0",
    "@openrouter/sdk": "^0.3.12",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "clipboardy": "^4.0.0",
    "conf": "^13.0.0",
    "execa": "^9.0.0",
    "ink": "^6.0.0",
    "marked": "^15.0.12",
    "openai": "^6.16.0",
    "react": "^19.0.0",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "@napi-rs/keyring-darwin-arm64": "^1.2.0",
    "@napi-rs/keyring-darwin-x64": "^1.2.0",
    "@napi-rs/keyring-linux-arm64-gnu": "^1.2.0",
    "@napi-rs/keyring-linux-arm64-musl": "^1.2.0",
    "@napi-rs/keyring-linux-x64-gnu": "^1.2.0",
    "@napi-rs/keyring-linux-x64-musl": "^1.2.0",
    "@napi-rs/keyring-win32-arm64-msvc": "^1.2.0",
    "@napi-rs/keyring-win32-ia32-msvc": "^1.2.0",
    "@napi-rs/keyring-win32-x64-msvc": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "minimatch": ">=10.2.1"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.5.0",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.17"
  }
}
